{
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-output-directory=tex-output",
                "-aux-directory=tex-output",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-aux-directory=tex-output",
                "-output-directory=tex-output",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "cSpell.words": [
        "ANNs",
        "Agrawal",
        "Brownlee",
        "Chablani",
        "LSTM",
        "Mailloux",
        "NLTK",
        "SVMs",
        "Shelopugin",
        "Sweta",
        "YRBSS",
        "posteriori"
    ],
    "latex-workshop.latex.outDir": "./tex-output",
    "python.venvPath": "~/pythonenv",
    "python.pythonPath": "~/pythonenv/CAP6640_PROJ/bin/python"
}
